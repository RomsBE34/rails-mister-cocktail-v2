


<h1>Cocktails#new</h1>

<div class="cocktail-form">
  <%= simple_form_for(@cocktail) do |f| %>
    <%= f.input :name %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>
</div>

<%= link_to "Home", cocktails_path %>



